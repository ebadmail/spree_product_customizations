<%= render partial: "spree/admin/shared/product_tabs", locals: { current: :customization_types } %>
<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @product } %>

<% content_for :page_actions do %>
  <%= button_link_to Spree.t(:add_product_customization_type), available_admin_product_product_customization_types_path(@product), remote: true, icon: "add", :'data-target' => "tbody#product_properties", class: "btn-success spree_add_fields" %>
<% end %>

<div id="selected-customization-types">
  <%= render "selected" %>
</div>

<div id="customization-types"></div>
