<% content_for :page_title do %>
  <%= t("new_product_customization_type") %>
<% end %>

<% content_for :page_actions do %>
  <%= link_to_with_icon 'arrow-left', t("back_to_customization_types_list"), collection_url, class: 'btn btn-default' %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @product_customization_type } %>

<%= form_for [:admin, @product_customization_type] do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>
